FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      thermo:relHum;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.2;

boundaryField
{
    floor
    {
        type            fixedHumidity;
        value           uniform 0;
    }

    ceiling
    {
        type            ho;
        value           uniform 0.5;
    }

    fixedWalls
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
