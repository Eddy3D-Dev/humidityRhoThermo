FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 305;

boundaryField
{
    walls 
    {
        type            zeroGradient;
    }

    inletTop
    {
        type            fixedValue;
        value           uniform 305;
    }

    inletBottom
    {
        type            fixedValue;
        value           uniform 305;
    }

    outletTop
    {
        type            inletOutlet;
        inletValue      uniform 305;
        value           uniform 305;
    }

    outletBottom
    {
        type            inletOutlet;
        inletValue      uniform 305;
        value           uniform 305;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }
}

// ************************************************************************* //
