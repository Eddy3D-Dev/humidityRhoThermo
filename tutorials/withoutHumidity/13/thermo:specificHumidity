FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "13";
    object      thermo:specificHumidity;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inletTop
    {
        type            fixedValue;
        value           uniform 0;
    }
    inletBottom
    {
        type            fixedValue;
        value           uniform 0;
    }
    outletTop
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    outletBottom
    {
        type            zeroGradient;
    }
}


